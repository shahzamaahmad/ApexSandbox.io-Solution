public Integer nextPrime(Integer num) {
    public static Boolean isPrime(Integer n){

    for (Integer i = 2; i <= n; i++){
    if(Math.mod(n,2)==0) return false;
    }
    return true;
    }

    public Integer nextPrime(Integer num) {
        Integer nextNum = num + 1;  // Start with the next number

        // Keep checking the next number until a prime is found
        while (true) {
            if (isPrime(nextNum)) {
                return nextNum;  // Return the next prime number
            }
            nextNum++;  // Increment to check the next number
        }
    }
}
