public Integer nextPrime(Integer num) {
public static Boolean isPrime(Integer n){
    if(Math.mod(n,2)==0){
        return false;
    }
    return true;
    }

    Integer nextNum = num+1;
    for (Integer i = 2; i <= nextNum; i++) {
        if (isPrime(nextNum)){
            return nextNum;
        };
        nextNum++;
    }
    return nextNum;
}

