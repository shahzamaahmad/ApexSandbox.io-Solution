public Integer nextPrime(Integer num) {
    public static Boolean isPrime(Integer n){

    for (Integer i = 2; i <= n; i++){
    if(Math.mod(n,2)==0) return false;
    }
    return true;
    }

    Integer nextNum = num+1;
    while (true) {
        if (isPrime(nextNum)) {
            return nextNum;
        }
        nextNum++;
    }
}
