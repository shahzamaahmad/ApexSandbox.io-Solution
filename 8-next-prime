public Integer nextPrime(Integer num) {

    Integer nextNum = num + 1;
        while (true) {
            if (isPrime(nextNum)) {
                return nextNum;
            }
        nextNum++;
        }

    
    public static Boolean isPrime(Integer n){
    for (Integer i = 2; i <= n-1; i++){
    if(Math.mod(n,i)==0) {
        return false; 
    }
    }
    return true;
    }

    
}

